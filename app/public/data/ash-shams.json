  "bismillah_pre": true,
  "name_simple": "Ash-Shams",
  "name_complex": "Ash-Shams",
  "name_arabic": "الشمس",
  "verses_count": 15,
  "pages": [
    595,
    595
  ],
  "slug": {
    "slug": "ash-shams",
    "locale": "en"
  },
  "translated_name": {
    "language_name": "indonesian",
    "name": "Matahari"
  },
  "verses": [
    {
      "id": 6044,
      "verse_number": 1,
      "verse_key": "91:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " وَٱلشَّمْسِ وَضُحَىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181117,
          "resource_id": 33,
          "text": "Demi matahari dan sinarnya pada pagi hari,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6045,
      "verse_number": 2,
      "verse_key": "91:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْقَمَرِ إِذَا تَلَىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181131,
          "resource_id": 33,
          "text": "demi bulan apabila mengiringinya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6046,
      "verse_number": 3,
      "verse_key": "91:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 183013,
          "resource_id": 33,
          "text": "demi siang apabila menampakkannya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6047,
      "verse_number": 4,
      "verse_key": "91:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّيْلِ إِذَا يَغْشَىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181144,
          "resource_id": 33,
          "text": "demi malam apabila menutupinya (gelap gulita),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6048,
      "verse_number": 5,
      "verse_key": "91:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 177506,
          "resource_id": 33,
          "text": "demi langit serta perancangannya (yang menakjubkan),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6049,
      "verse_number": 6,
      "verse_key": "91:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْأَرْضِ وَمَا طَحَىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181149,
          "resource_id": 33,
          "text": "demi bumi serta penghamparannya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6050,
      "verse_number": 7,
      "verse_key": "91:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَنَفْسٍ وَمَا سَوَّىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181146,
          "resource_id": 33,
          "text": "demi jiwa serta penyempurnaan (ciptaan)nya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6051,
      "verse_number": 8,
      "verse_key": "91:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181151,
          "resource_id": 33,
          "text": "maka Dia mengilhamkan kepadanya (jalan) kejahatan dan ketakwaannya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6052,
      "verse_number": 9,
      "verse_key": "91:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قَدْ أَفْلَحَ مَن زَكَّىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181153,
          "resource_id": 33,
          "text": "sungguh beruntung orang yang menyucikannya (jiwa itu),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6053,
      "verse_number": 10,
      "verse_key": "91:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَقَدْ خَابَ مَن دَسَّىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181114,
          "resource_id": 33,
          "text": "dan sungguh rugi orang yang mengotorinya."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6054,
      "verse_number": 11,
      "verse_key": "91:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَذَّبَتْ ثَمُودُ بِطَغْوَىٰهَآ",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 177504,
          "resource_id": 33,
          "text": "(Kaum) Samud telah mendustakan (rasulnya) karena mereka melampaui batas (zalim),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6055,
      "verse_number": 12,
      "verse_key": "91:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِذِ ٱنۢبَعَثَ أَشْقَىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181119,
          "resource_id": 33,
          "text": "ketika bangkit orang yang paling celaka di antara mereka,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6056,
      "verse_number": 13,
      "verse_key": "91:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقَالَ لَهُمْ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقْيَـٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181121,
          "resource_id": 33,
          "text": "lalu Rasul Allah (Saleh) berkata kepada mereka, \"(Biarkanlah) unta betina dari Allah ini dengan minumannya.\""
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6057,
      "verse_number": 14,
      "verse_key": "91:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنۢبِهِمْ فَسَوَّىٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181126,
          "resource_id": 33,
          "text": "Namun mereka mendustakannya dan menyembelihnya, karena itu Tuhan membinasakan mereka karena dosanya, lalu diratakan-Nya (dengan tanah)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6058,
      "verse_number": 15,
      "verse_key": "91:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَا يَخَافُ عُقْبَـٰهَا",
      "page_number": 595,
      "juz_number": 30,
      "translations": [
        {
          "id": 181123,
          "resource_id": 33,
          "text": "Dan Dia tidak takut terhadap akibatnya."
        }
      ],
      "foot_notes": []
    }
  ]
}