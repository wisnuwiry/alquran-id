    "language_name": "indonesian",
    "name": "Gajah"
  },
  "verses": [
    {
      "id": 6189,
      "verse_number": 1,
      "verse_key": "105:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 549,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَـٰبِ ٱلْفِيلِ",
      "page_number": 601,
      "juz_number": 30,
      "translations": [
        {
          "id": 181373,
          "resource_id": 33,
          "text": "Tidakkah engkau (Muhammad) perhatikan bagaimana Tuhanmu telah bertindak terhadap pasukan bergajah?<sup foot_note=135876>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135876,
            "text": "* Pasukan yang dipimpin oleh Abrahah, Gubernur Yaman yang hendak menghancurkan Ka'bah. Sebelum masuk ke kota Mekkah, pasukan tersebut diserang burung-burung yang melemparinya dengan batu-batu kecil sehingga mereka musnah.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 6190,
      "verse_number": 2,
      "verse_key": "105:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 549,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِى تَضْلِيلٍ",
      "page_number": 601,
      "juz_number": 30,
      "translations": [
        {
          "id": 181354,
          "resource_id": 33,
          "text": "Bukankah Dia telah menjadikan tipu daya mereka itu sia-sia?"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6191,
      "verse_number": 3,
      "verse_key": "105:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 549,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ",
      "page_number": 601,
      "juz_number": 30,
      "translations": [
        {
          "id": 181379,
          "resource_id": 33,
          "text": "dan Dia mengirimkan kepada mereka burung yang berbondong-bondong,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6192,
      "verse_number": 4,
      "verse_key": "105:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 549,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَرْمِيهِم بِحِجَارَةٍ مِّن سِجِّيلٍ",
      "page_number": 601,
      "juz_number": 30,
      "translations": [
        {
          "id": 181390,
          "resource_id": 33,
          "text": "yang melempari mereka dengan batu dari tanah liat yang dibakar,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6193,
      "verse_number": 5,
      "verse_key": "105:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 549,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَجَعَلَهُمْ كَعَصْفٍ مَّأْكُولٍۭ",
      "page_number": 601,
      "juz_number": 30,
      "translations": [
        {
          "id": 181382,
          "resource_id": 33,
          "text": "sehingga mereka dijadikan-Nya seperti daun-daun yang dimakan (ulat)."
        }
      ],
      "foot_notes": []
    }
  ]
}