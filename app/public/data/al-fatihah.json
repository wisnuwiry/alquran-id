  "translated_name": {
    "language_name": "indonesian",
    "name": "Pembukaan"
  },
  "verses": [
    {
      "id": 1,
      "verse_number": 1,
      "verse_key": "1:1",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "بِسْمِ ٱللَّهِ ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 181444,
          "resource_id": 33,
          "text": "Dengan nama Allah Yang Maha Pengasih, Maha Penyayang."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 2,
      "verse_number": 2,
      "verse_key": "1:2",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "ٱلْحَمْدُ لِلَّهِ رَبِّ ٱلْعَـٰلَمِينَ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 177234,
          "resource_id": 33,
          "text": "Segala puji bagi Allah, Tuhan seluruh alam,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 3,
      "verse_number": 3,
      "verse_key": "1:3",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "ٱلرَّحْمَـٰنِ ٱلرَّحِيمِ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 177531,
          "resource_id": 33,
          "text": "Yang Maha Pengasih, Maha Penyayang,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 4,
      "verse_number": 4,
      "verse_key": "1:4",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "مَـٰلِكِ يَوْمِ ٱلدِّينِ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 182987,
          "resource_id": 33,
          "text": "Pemilik hari pembalasan.<sup foot_note=134952>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 134952,
            "text": "* Yaumiddīn (hari pembalasan), hari waktu manusia menerima pembalasan amalnya, baik atau buruk. Disebut juga yaumul qiyāmah, yaumul hisab dan sebagainya.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5,
      "verse_number": 5,
      "verse_key": "1:5",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 182918,
          "resource_id": 33,
          "text": "Hanya kepada Engkaulah kami menyembah dan hanya kepada Engkaulah kami mohon pertolongan."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6,
      "verse_number": 6,
      "verse_key": "1:6",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "ٱهْدِنَا ٱلصِّرَٰطَ ٱلْمُسْتَقِيمَ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 181877,
          "resource_id": 33,
          "text": "Tunjukilah kami jalan yang lurus,<sup foot_note=134953>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 134953,
            "text": "* Jalan yang lurus, yaitu jalan hidup yang benar, yang dapat membuat bahagia di dunia dan di akhirat.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 7,
      "verse_number": 7,
      "verse_key": "1:7",
      "hizb_number": 1,
      "rub_el_hizb_number": 1,
      "ruku_number": 1,
      "manzil_number": 1,
      "sajdah_number": null,
      "text_uthmani": "صِرَٰطَ ٱلَّذِينَ أَنْعَمْتَ عَلَيْهِمْ غَيْرِ ٱلْمَغْضُوبِ عَلَيْهِمْ وَلَا ٱلضَّآلِّينَ",
      "page_number": 1,
      "juz_number": 1,
      "translations": [
        {
          "id": 182567,
          "resource_id": 33,
          "text": "(yaitu) jalan orang-orang yang telah Engkau beri nikmat kepadanya; bukan (jalan) mereka yang dimurkai, dan bukan (pula jalan) mereka yang sesat.<sup foot_note=134954>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 134954,
            "text": "* Mereka yang dimurkai, adalah mereka yang sengaja menentang ajaran Islam. Mereka yang sesat adalah mereka yang sengaja mengambil jalan lain selain ajaran Islam.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    }
  ]
}