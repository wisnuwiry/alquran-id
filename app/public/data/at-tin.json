  ],
  "slug": {
    "slug": "at-tin",
    "locale": "en"
  },
  "translated_name": {
    "language_name": "indonesian",
    "name": "Buah Tin"
  },
  "verses": [
    {
      "id": 6099,
      "verse_number": 1,
      "verse_key": "95:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلتِّينِ وَٱلزَّيْتُونِ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 181223,
          "resource_id": 33,
          "text": "Demi (buah) Tin dan (buah) Zaitun,<sup foot_note=135867>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135867,
            "text": "* Yang dimaksud dengan \"Tin\" oleh sebagian mufasir ialah tempat tinggal Nabi Nuh -'alaihissalām-, yaitu Damaskus yang banyak tumbuh pohon Tin; dan \"zaitun\" ialah Baitulmaqdis yang banyak tumbuh zaitun.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 6100,
      "verse_number": 2,
      "verse_key": "95:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَطُورِ سِينِينَ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 181231,
          "resource_id": 33,
          "text": "demi gunung Sinai, <sup foot_note=135868>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135868,
            "text": "* Gunung Sinai adalah tempat Nabi Musa -'alaihissalām- menerima wahyu.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 6101,
      "verse_number": 3,
      "verse_key": "95:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَهَـٰذَا ٱلْبَلَدِ ٱلْأَمِينِ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 181233,
          "resource_id": 33,
          "text": "dan demi negeri (Mekkah) yang aman ini."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6102,
      "verse_number": 4,
      "verse_key": "95:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ فِىٓ أَحْسَنِ تَقْوِيمٍ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 181235,
          "resource_id": 33,
          "text": "Sungguh, Kami telah menciptakan manusia dalam bentuk yang sebaik-baiknya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6103,
      "verse_number": 5,
      "verse_key": "95:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ رَدَدْنَـٰهُ أَسْفَلَ سَـٰفِلِينَ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 181236,
          "resource_id": 33,
          "text": "kemudian Kami kembalikan dia ke tempat yang serendah-rendahnya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6104,
      "verse_number": 6,
      "verse_key": "95:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 181238,
          "resource_id": 33,
          "text": "kecuali orang-orang yang beriman dan mengerjakan kebajikan; maka mereka akan mendapat pahala yang tidak ada putus-putusnya."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6105,
      "verse_number": 7,
      "verse_key": "95:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَمَا يُكَذِّبُكَ بَعْدُ بِٱلدِّينِ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 181240,
          "resource_id": 33,
          "text": "Maka apa yang menyebabkan (mereka) mendustakanmu (tentang) hari pembalasan setelah (adanya keterangan-keterangan) itu?"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6106,
      "verse_number": 8,
      "verse_key": "95:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 539,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَيْسَ ٱللَّهُ بِأَحْكَمِ ٱلْحَـٰكِمِينَ",
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 181245,
          "resource_id": 33,
          "text": "Bukankah Allah Hakim yang paling adil?"
        }
      ],
      "foot_notes": []
    }
  ]
}