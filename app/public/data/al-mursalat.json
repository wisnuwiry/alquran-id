{
  "id": 77,
  "revelation_place": "makkah",
  "revelation_order": 33,
  "bismillah_pre": true,
  "name_simple": "Al-Mursalat",
  "name_complex": "Al-Mursalāt",
  "name_arabic": "المرسلات",
  "verses_count": 50,
  "pages": [
    580,
    581
  ],
  "slug": {
    "slug": "al-mursalat",
    "locale": "en"
  },
  "translated_name": {
    "language_name": "indonesian",
    "name": "Malaikat-malaikan yang Diutus"
  },
  "verses": [
    {
      "id": 5623,
      "verse_number": 1,
      "verse_key": "77:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " وَٱلْمُرْسَلَـٰتِ عُرْفًا",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180605,
          "resource_id": 33,
          "text": "Demi (malaikat-malaikat)<sup foot_note=135836>1</sup> yang diutus untuk membawa kebaikan,"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135836,
            "text": "* Sebagian mufasir mengartikan “Demi angin yang dikirim.”",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5624,
      "verse_number": 2,
      "verse_key": "77:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْعَـٰصِفَـٰتِ عَصْفًا",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180625,
          "resource_id": 33,
          "text": "dan (malaikat-malaikat) yang terbang dengan kencangnya,<sup foot_note=135837>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135837,
            "text": "* Terbang untuk melaksanakan perintah Allah.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5625,
      "verse_number": 3,
      "verse_key": "77:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلنَّـٰشِرَٰتِ نَشْرًا",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180697,
          "resource_id": 33,
          "text": "dan (malaikat-malaikat) yang menyebarkan (rahmat Allah) dengan seluas-luasnya,<sup foot_note=135838>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135838,
            "text": "* Pada waktu malaikat turun untuk membawa wahyu, sebagian mufasir berpendapat, bahwa yang dimaksud dengan an-nāsyirāt ialah angin yang bertiup disertai hujan.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5626,
      "verse_number": 4,
      "verse_key": "77:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْفَـٰرِقَـٰتِ فَرْقًا",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180644,
          "resource_id": 33,
          "text": "dan (malaikat-malaikat) yang membedakan (antara yang baik dan yang buruk) dengan sejelas-jelasnya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5627,
      "verse_number": 5,
      "verse_key": "77:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْمُلْقِيَـٰتِ ذِكْرًا",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180651,
          "resource_id": 33,
          "text": "dan (malaikat-malaikat) yang menyampaikan wahyu,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5628,
      "verse_number": 6,
      "verse_key": "77:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عُذْرًا أَوْ نُذْرًا",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180653,
          "resource_id": 33,
          "text": "untuk menolak alasan-alasan atau memberi peringatan."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5629,
      "verse_number": 7,
      "verse_key": "77:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّمَا تُوعَدُونَ لَوَٰقِعٌ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180655,
          "resource_id": 33,
          "text": "Sungguh, apa yang dijanjikan kepadamu pasti terjadi."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5630,
      "verse_number": 8,
      "verse_key": "77:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا ٱلنُّجُومُ طُمِسَتْ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180657,
          "resource_id": 33,
          "text": "Maka apabila bintang-bintang dihapuskan,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5631,
      "verse_number": 9,
      "verse_key": "77:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلسَّمَآءُ فُرِجَتْ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 179889,
          "resource_id": 33,
          "text": "dan apabila langit terbelah,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5632,
      "verse_number": 10,
      "verse_key": "77:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْجِبَالُ نُسِفَتْ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180699,
          "resource_id": 33,
          "text": "dan apabila gunung-gunung dihancurkan menjadi debu,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5633,
      "verse_number": 11,
      "verse_key": "77:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلرُّسُلُ أُقِّتَتْ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180627,
          "resource_id": 33,
          "text": "dan apabila rasul-rasul telah ditetapkan waktunya.<sup foot_note=135839>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135839,
            "text": "* Waktu untuk berkumpul bersama umat mereka masing-masing.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5634,
      "verse_number": 12,
      "verse_key": "77:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِأَىِّ يَوْمٍ أُجِّلَتْ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180606,
          "resource_id": 33,
          "text": "(Niscaya dikatakan kepada mereka), \"Sampai hari apakah ditangguhkan (azab orang-orang kafir itu)?\""
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5635,
      "verse_number": 13,
      "verse_key": "77:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِيَوْمِ ٱلْفَصْلِ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180607,
          "resource_id": 33,
          "text": "Sampai hari keputusan."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5636,
      "verse_number": 14,
      "verse_key": "77:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180614,
          "resource_id": 33,
          "text": "Dan tahukah kamu apakah hari keputusan itu?"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5637,
      "verse_number": 15,
      "verse_key": "77:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180608,
          "resource_id": 33,
          "text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5638,
      "verse_number": 16,
      "verse_key": "77:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180609,
          "resource_id": 33,
          "text": "Bukankah telah Kami binasakan orang-orang yang dahulu?"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5639,
      "verse_number": 17,
      "verse_key": "77:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ نُتْبِعُهُمُ ٱلْـَٔاخِرِينَ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180610,
          "resource_id": 33,
          "text": "Lalu Kami susulkan (azab Kami terhadap) orang-orang yang datang kemudian."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5640,
      "verse_number": 18,
      "verse_key": "77:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180611,
          "resource_id": 33,
          "text": "Demikianlah Kami perlakukan orang-orang yang berdosa."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5641,
      "verse_number": 19,
      "verse_key": "77:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 180612,
          "resource_id": 33,
          "text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5642,
      "verse_number": 20,
      "verse_key": "77:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ نَخْلُقكُّم مِّن مَّآءٍ مَّهِينٍ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180615,
          "resource_id": 33,
          "text": "Bukankah Kami menciptakan kamu dari air yang hina (mani),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5643,
      "verse_number": 21,
      "verse_key": "77:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَجَعَلْنَـٰهُ فِى قَرَارٍ مَّكِينٍ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180616,
          "resource_id": 33,
          "text": "kemudian Kami letakkan ia dalam tempat yang kokoh (rahim),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5644,
      "verse_number": 22,
      "verse_key": "77:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَىٰ قَدَرٍ مَّعْلُومٍ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180617,
          "resource_id": 33,
          "text": "sampai waktu yang ditentukan,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5645,
      "verse_number": 23,
      "verse_key": "77:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقَدَرْنَا فَنِعْمَ ٱلْقَـٰدِرُونَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180618,
          "resource_id": 33,
          "text": "lalu Kami tentukan (bentuknya), maka (Kamilah) sebaik-baik yang menentukan."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5646,
      "verse_number": 24,
      "verse_key": "77:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180619,
          "resource_id": 33,
          "text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5647,
      "verse_number": 25,
      "verse_key": "77:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180620,
          "resource_id": 33,
          "text": "Bukankah Kami jadikan bumi untuk (tempat) berkumpul,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5648,
      "verse_number": 26,
      "verse_key": "77:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَحْيَآءً وَأَمْوَٰتًا",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180621,
          "resource_id": 33,
          "text": "bagi yang masih hidup dan yang sudah mati?<sup foot_note=135840>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135840,
            "text": "* Bumi mengumpulkan orang-orang hidup di permukaannya dan orang-orang mati dalam perutnya.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5649,
      "verse_number": 27,
      "verse_key": "77:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَـٰمِخَـٰتٍ وَأَسْقَيْنَـٰكُم مَّآءً فُرَاتًا",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180622,
          "resource_id": 33,
          "text": "Dan Kami jadikan padanya gunung-gunung yang tinggi, dan Kami beri minum kamu dengan air tawar?"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5650,
      "verse_number": 28,
      "verse_key": "77:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180623,
          "resource_id": 33,
          "text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5651,
      "verse_number": 29,
      "verse_key": "77:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180624,
          "resource_id": 33,
          "text": "(Akan dikatakan), \"Pergilah kamu guna mendapatkan apa (azab) yang dahulu kamu dustakan."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5652,
      "verse_number": 30,
      "verse_key": "77:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّ ذِى ثَلَـٰثِ شُعَبٍ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180637,
          "resource_id": 33,
          "text": "Pergilah kamu mendapatkan naungan (asap api neraka) yang mempunyai tiga cabang,<sup foot_note=135841>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135841,
            "text": "* Adalah yang mempunyai tiga gejolak, yaitu di kanan, di kiri dan di atas. Ini berarti bahwa azab itu mengepung orang-orang kafir dari segala penjuru.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5653,
      "verse_number": 31,
      "verse_key": "77:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَّا ظَلِيلٍ وَلَا يُغْنِى مِنَ ٱللَّهَبِ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180628,
          "resource_id": 33,
          "text": "yang tidak melindungi dan tidak pula menolak nyala api neraka.\""
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5654,
      "verse_number": 32,
      "verse_key": "77:32",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهَا تَرْمِى بِشَرَرٍ كَٱلْقَصْرِ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180629,
          "resource_id": 33,
          "text": "Sungguh, (neraka) itu menyemburkan bunga api (sebesar dan setinggi) istana,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5655,
      "verse_number": 33,
      "verse_key": "77:33",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَأَنَّهُۥ جِمَـٰلَتٌ صُفْرٌ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180630,
          "resource_id": 33,
          "text": "seakan-akan iring-iringan unta yang kuning."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5656,
      "verse_number": 34,
      "verse_key": "77:34",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180631,
          "resource_id": 33,
          "text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5657,
      "verse_number": 35,
      "verse_key": "77:35",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا يَوْمُ لَا يَنطِقُونَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180632,
          "resource_id": 33,
          "text": "Inilah hari, saat mereka tidak dapat berbicara,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5658,
      "verse_number": 36,
      "verse_key": "77:36",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 179993,
          "resource_id": 33,
          "text": "dan tidak diizinkan kepada mereka mengemukakan alasan agar mereka dimaafkan."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5659,
      "verse_number": 37,
      "verse_key": "77:37",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180633,
          "resource_id": 33,
          "text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5660,
      "verse_number": 38,
      "verse_key": "77:38",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَـٰكُمْ وَٱلْأَوَّلِينَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180634,
          "resource_id": 33,
          "text": "Inilah hari keputusan; (pada hari ini) Kami kumpulkan kamu dan orang-orang yang terdahulu."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5661,
      "verse_number": 39,
      "verse_key": "77:39",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِن كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180635,
          "resource_id": 33,
          "text": "Maka jika kamu punya tipu daya, maka lakukanlah (tipu daya) itu terhadap-Ku."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5662,
      "verse_number": 40,
      "verse_key": "77:40",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180638,
          "resource_id": 33,
          "text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5663,
      "verse_number": 41,
      "verse_key": "77:41",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلْمُتَّقِينَ فِى ظِلَـٰلٍ وَعُيُونٍ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 177482,
          "resource_id": 33,
          "text": "Sungguh, orang-orang yang bertakwa berada dalam naungan (pepohonan surga yang teduh) dan (di sekitar) mata air,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5664,
      "verse_number": 42,
      "verse_key": "77:42",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180639,
          "resource_id": 33,
          "text": "dan buah-buahan yang mereka sukai."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5665,
      "verse_number": 43,
      "verse_key": "77:43",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180640,
          "resource_id": 33,
          "text": "(Katakan kepada mereka), \"Makan dan minumlah dengan rasa nikmat sebagai balasan dari apa yang telah kamu kerjakan.\""
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5666,
      "verse_number": 44,
      "verse_key": "77:44",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180641,
          "resource_id": 33,
          "text": "Sungguh, demikianlah Kami memberi balasan kepada orang-orang yang berbuat baik."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5667,
      "verse_number": 45,
      "verse_key": "77:45",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180642,
          "resource_id": 33,
          "text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5668,
      "verse_number": 46,
      "verse_key": "77:46",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كُلُوا۟ وَتَمَتَّعُوا۟ قَلِيلًا إِنَّكُم مُّجْرِمُونَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180643,
          "resource_id": 33,
          "text": "(Katakan kepada orang-orang kafir), \"Makan dan bersenang-senanglah kamu (di dunia) sebentar, sesungguhnya kamu orang-orang durhaka!\""
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5669,
      "verse_number": 47,
      "verse_key": "77:47",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180648,
          "resource_id": 33,
          "text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5670,
      "verse_number": 48,
      "verse_key": "77:48",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا قِيلَ لَهُمُ ٱرْكَعُوا۟ لَا يَرْكَعُونَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180645,
          "resource_id": 33,
          "text": "Dan apabila dikatakan kepada mereka, \"Rukuklah,\" mereka tidak mau rukuk.<sup foot_note=135842>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135842,
            "text": "* Sebagian mufasir mengatakan, bahwa yang dimaksud dengan rukuk di sini ialah tunduk kepada perintah Allah; dan sebagian yang lainnya mengatakan, maksudnya ialah salat.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5671,
      "verse_number": 49,
      "verse_key": "77:49",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180646,
          "resource_id": 33,
          "text": "Celakalah pada hari itu, bagi mereka yang mendustakan (kebenaran)!"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5672,
      "verse_number": 50,
      "verse_key": "77:50",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَبِأَىِّ حَدِيثٍۭ بَعْدَهُۥ يُؤْمِنُونَ",
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 180649,
          "resource_id": 33,
          "text": "Maka kepada ajaran manakah (selain Alquran) ini mereka akan beriman?"
        }
      ],
      "foot_notes": []
    }
  ]
}