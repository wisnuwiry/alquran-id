{
  "id": 80,
  "revelation_place": "makkah",
  "revelation_order": 24,
  "bismillah_pre": true,
  "name_simple": "'Abasa",
  "name_complex": "`Abasa",
  "name_arabic": "عبس",
  "verses_count": 42,
  "pages": [
    585,
    585
  ],
  "slug": {
    "slug": "abasa",
    "locale": "en"
  },
  "translated_name": {
    "language_name": "indonesian",
    "name": "Ia Bermuka Masam"
  },
  "verses": [
    {
      "id": 5759,
      "verse_number": 1,
      "verse_key": "80:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " عَبَسَ وَتَوَلَّىٰٓ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180760,
          "resource_id": 33,
          "text": "Dia (Muhammad) berwajah masam dan berpaling,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5760,
      "verse_number": 2,
      "verse_key": "80:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَن جَآءَهُ ٱلْأَعْمَىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180769,
          "resource_id": 33,
          "text": "karena seorang buta telah datang kepadanya (Abdullah bin Ummi Maktum)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5761,
      "verse_number": 3,
      "verse_key": "80:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180777,
          "resource_id": 33,
          "text": "Dan tahukah engkau (Muhammad) barangkali dia ingin menyucikan dirinya (dari dosa),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5762,
      "verse_number": 4,
      "verse_key": "80:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكْرَىٰٓ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180789,
          "resource_id": 33,
          "text": "atau dia (ingin) mendapatkan pengajaran, yang memberi manfaat kepadanya?"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5763,
      "verse_number": 5,
      "verse_key": "80:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَمَّا مَنِ ٱسْتَغْنَىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180793,
          "resource_id": 33,
          "text": "Adapun orang yang merasa dirinya serba cukup (pembesar-pembesar Quraisy),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5764,
      "verse_number": 6,
      "verse_key": "80:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180794,
          "resource_id": 33,
          "text": "maka engkau (Muhammad) memberi perhatian kepadanya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5765,
      "verse_number": 7,
      "verse_key": "80:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180795,
          "resource_id": 33,
          "text": "padahal tidak ada (cela) atasmu kalau dia tidak menyucikan diri (beriman)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5766,
      "verse_number": 8,
      "verse_key": "80:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَمَّا مَن جَآءَكَ يَسْعَىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180796,
          "resource_id": 33,
          "text": "Dan adapun orang yang datang kepadamu dengan bersegera (untuk mendapatkan pengajaran),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5767,
      "verse_number": 9,
      "verse_key": "80:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَهُوَ يَخْشَىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180797,
          "resource_id": 33,
          "text": "sedang dia takut (kepada Allah),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5768,
      "verse_number": 10,
      "verse_key": "80:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَنتَ عَنْهُ تَلَهَّىٰ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180801,
          "resource_id": 33,
          "text": "engkau (Muhammad) malah mengabaikannya."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5769,
      "verse_number": 11,
      "verse_key": "80:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّآ إِنَّهَا تَذْكِرَةٌ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180799,
          "resource_id": 33,
          "text": "Sekali-kali jangan (begitu)! Sungguh, (ajaran-ajaran Allah) itu suatu peringatan,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5770,
      "verse_number": 12,
      "verse_key": "80:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَمَن شَآءَ ذَكَرَهُۥ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180761,
          "resource_id": 33,
          "text": "maka barangsiapa menghendaki, tentulah dia akan memperhatikannya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5771,
      "verse_number": 13,
      "verse_key": "80:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِى صُحُفٍ مُّكَرَّمَةٍ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180762,
          "resource_id": 33,
          "text": "di dalam kitab-kitab yang dimuliakan (di sisi Allah),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5772,
      "verse_number": 14,
      "verse_key": "80:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَّرْفُوعَةٍ مُّطَهَّرَةٍۭ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180766,
          "resource_id": 33,
          "text": "yang ditinggikan (dan) disucikan,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5773,
      "verse_number": 15,
      "verse_key": "80:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بِأَيْدِى سَفَرَةٍ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180763,
          "resource_id": 33,
          "text": "di tangan para utusan (malaikat),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5774,
      "verse_number": 16,
      "verse_key": "80:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كِرَامٍۭ بَرَرَةٍ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180764,
          "resource_id": 33,
          "text": "yang mulia lagi berbakti."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5775,
      "verse_number": 17,
      "verse_key": "80:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قُتِلَ ٱلْإِنسَـٰنُ مَآ أَكْفَرَهُۥ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180765,
          "resource_id": 33,
          "text": "Celakalah manusia! Alangkah kufurnya dia!"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5776,
      "verse_number": 18,
      "verse_key": "80:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مِنْ أَىِّ شَىْءٍ خَلَقَهُۥ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180768,
          "resource_id": 33,
          "text": "Dari apakah Dia (Allah) menciptakannya?"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5777,
      "verse_number": 19,
      "verse_key": "80:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180767,
          "resource_id": 33,
          "text": "Dari setetes mani, Dia menciptakannya lalu menentukannya.<sup foot_note=135848>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135848,
            "text": "* Menentukan fase-fase kejadiannya, umurnya, rezekinya dan nasibnya.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5778,
      "verse_number": 20,
      "verse_key": "80:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180770,
          "resource_id": 33,
          "text": "Kemudian jalannya Dia mudahkan,<sup foot_note=135849>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135849,
            "text": "* Memudahkan kelahirannya atau memberi persediaan kepadanya untuk menjalani jalan yang benar atau jalan yang sesat.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5779,
      "verse_number": 21,
      "verse_key": "80:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180771,
          "resource_id": 33,
          "text": "kemudian Dia mematikannya lalu menguburkannya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5780,
      "verse_number": 22,
      "verse_key": "80:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180772,
          "resource_id": 33,
          "text": "kemudian jika Dia menghendaki, Dia membangkitkannya kembali."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5781,
      "verse_number": 23,
      "verse_key": "80:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180773,
          "resource_id": 33,
          "text": "Sekali-kali jangan (begitu)! Dia (manusia) itu belum melaksanakan apa yang Dia (Allah) perintahkan kepadanya."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5782,
      "verse_number": 24,
      "verse_key": "80:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلْيَنظُرِ ٱلْإِنسَـٰنُ إِلَىٰ طَعَامِهِۦٓ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180774,
          "resource_id": 33,
          "text": "Maka hendaklah manusia itu memperhatikan makanannya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5783,
      "verse_number": 25,
      "verse_key": "80:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَنَّا صَبَبْنَا ٱلْمَآءَ صَبًّا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180775,
          "resource_id": 33,
          "text": "Kamilah yang telah mencurahkan air melimpah (dari langit),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5784,
      "verse_number": 26,
      "verse_key": "80:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ شَقَقْنَا ٱلْأَرْضَ شَقًّا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180776,
          "resource_id": 33,
          "text": "kemudian Kami belah bumi dengan sebaik-baiknya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5785,
      "verse_number": 27,
      "verse_key": "80:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَنۢبَتْنَا فِيهَا حَبًّا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 179981,
          "resource_id": 33,
          "text": "lalu di sana Kami tumbuhkan biji-bijian,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5786,
      "verse_number": 28,
      "verse_key": "80:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَعِنَبًا وَقَضْبًا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 179982,
          "resource_id": 33,
          "text": "dan anggur dan sayur-sayuran,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5787,
      "verse_number": 29,
      "verse_key": "80:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَزَيْتُونًا وَنَخْلًا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180778,
          "resource_id": 33,
          "text": "dan zaitun dan pohon kurma,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5788,
      "verse_number": 30,
      "verse_key": "80:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَحَدَآئِقَ غُلْبًا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180780,
          "resource_id": 33,
          "text": "dan kebun-kebun (yang) rindang,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5789,
      "verse_number": 31,
      "verse_key": "80:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفَـٰكِهَةً وَأَبًّا",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180779,
          "resource_id": 33,
          "text": "dan buah-buahan serta rerumputan."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5790,
      "verse_number": 32,
      "verse_key": "80:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَّتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180781,
          "resource_id": 33,
          "text": "(Semua itu) untuk kesenanganmu dan untuk hewan-hewan ternakmu."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5791,
      "verse_number": 33,
      "verse_key": "80:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180782,
          "resource_id": 33,
          "text": "Maka apabila datang suara yang memekakkan (tiupan sangkakala yang kedua),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5792,
      "verse_number": 34,
      "verse_key": "80:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180783,
          "resource_id": 33,
          "text": "pada hari itu manusia lari dari saudaranya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5793,
      "verse_number": 35,
      "verse_key": "80:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأُمِّهِۦ وَأَبِيهِ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180784,
          "resource_id": 33,
          "text": "dan dari ibu dan bapaknya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5794,
      "verse_number": 36,
      "verse_key": "80:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَصَـٰحِبَتِهِۦ وَبَنِيهِ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180786,
          "resource_id": 33,
          "text": "dan dari istri dan anak-anaknya."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5795,
      "verse_number": 37,
      "verse_key": "80:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِكُلِّ ٱمْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180785,
          "resource_id": 33,
          "text": "Setiap orang dari mereka pada hari itu mempunyai urusan yang menyibukkannya."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5796,
      "verse_number": 38,
      "verse_key": "80:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180787,
          "resource_id": 33,
          "text": "Pada hari itu ada wajah-wajah yang berseri-seri,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5797,
      "verse_number": 39,
      "verse_key": "80:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180788,
          "resource_id": 33,
          "text": "tertawa dan gembira ria,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5798,
      "verse_number": 40,
      "verse_key": "80:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ",
      "page_number": 585,
      "juz_number": 30,
      "translations": [
        {
          "id": 180790,
          "resource_id": 33,
          "text": "dan pada hari itu ada (pula) wajah-wajah yang tertutup debu (suram),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5799,
      "verse_number": 41,
      "verse_key": "80:41",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَرْهَقُهَا قَتَرَةٌ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 180791,
          "resource_id": 33,
          "text": "tertutup oleh kegelapan (ditimpa kehinaan dan kesusahan)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5800,
      "verse_number": 42,
      "verse_key": "80:42",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أُو۟لَـٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 180792,
          "resource_id": 33,
          "text": "Mereka itulah orang-orang kafir yang durhaka."
        }
      ],
      "foot_notes": []
    }
  ]
}