  "translated_name": {
    "language_name": "indonesian",
    "name": "Gejolak Api (Sabut)"
  },
  "verses": [
    {
      "id": 6217,
      "verse_number": 1,
      "verse_key": "111:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 555,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " تَبَّتْ يَدَآ أَبِى لَهَبٍ وَتَبَّ",
      "page_number": 603,
      "juz_number": 30,
      "translations": [
        {
          "id": 181462,
          "resource_id": 33,
          "text": "Binasalah kedua tangan Abu Lahab dan benar-benar binasa dia!<sup foot_note=135881>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135881,
            "text": "* Yang dimaksud dengan \"kedua tangan Abu Lahab\" ialah Abu Lahab.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 6218,
      "verse_number": 2,
      "verse_key": "111:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 555,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَآ أَغْنَىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَ",
      "page_number": 603,
      "juz_number": 30,
      "translations": [
        {
          "id": 177230,
          "resource_id": 33,
          "text": "Tidaklah berguna baginya hartanya dan apa yang dia usahakan."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6219,
      "verse_number": 3,
      "verse_key": "111:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 555,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "سَيَصْلَىٰ نَارًا ذَاتَ لَهَبٍ",
      "page_number": 603,
      "juz_number": 30,
      "translations": [
        {
          "id": 181484,
          "resource_id": 33,
          "text": "Kelak dia akan masuk ke dalam api yang bergejolak (neraka)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6220,
      "verse_number": 4,
      "verse_key": "111:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 555,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ",
      "page_number": 603,
      "juz_number": 30,
      "translations": [
        {
          "id": 181486,
          "resource_id": 33,
          "text": "Dan (begitu pula) istrinya, pembawa kayu bakar (penyebar fitnah)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 6221,
      "verse_number": 5,
      "verse_key": "111:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 555,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِى جِيدِهَا حَبْلٌ مِّن مَّسَدٍۭ",
      "page_number": 603,
      "juz_number": 30,
      "translations": [
        {
          "id": 177415,
          "resource_id": 33,
          "text": "Di lehernya ada tali dari sabut yang dipintal."
        }
      ],
      "foot_notes": []
    }
  ]
}