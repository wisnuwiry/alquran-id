{
  "id": 87,
  "revelation_place": "makkah",
  "revelation_order": 8,
  "bismillah_pre": true,
  "name_simple": "Al-A'la",
  "name_complex": "Al-'A`lá",
  "name_arabic": "الأعلى",
  "verses_count": 19,
  "pages": [
    591,
    592
  ],
  "slug": {
    "slug": "al-ala",
    "locale": "en"
  },
  "translated_name": {
    "language_name": "indonesian",
    "name": "Yang Paling Tinggi"
  },
  "verses": [
    {
      "id": 5949,
      "verse_number": 1,
      "verse_key": "87:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى",
      "page_number": 591,
      "juz_number": 30,
      "translations": [
        {
          "id": 177496,
          "resource_id": 33,
          "text": "Sucikanlah nama Tuhanmu Yang Mahatinggi,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5950,
      "verse_number": 2,
      "verse_key": "87:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
      "page_number": 591,
      "juz_number": 30,
      "translations": [
        {
          "id": 181017,
          "resource_id": 33,
          "text": "yang menciptakan, lalu menyempurnakan (penciptaan-Nya),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5951,
      "verse_number": 3,
      "verse_key": "87:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِى قَدَّرَ فَهَدَىٰ",
      "page_number": 591,
      "juz_number": 30,
      "translations": [
        {
          "id": 181019,
          "resource_id": 33,
          "text": "yang menentukan kadar (masing-masing) dan memberi petunjuk,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5952,
      "verse_number": 4,
      "verse_key": "87:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ",
      "page_number": 591,
      "juz_number": 30,
      "translations": [
        {
          "id": 181021,
          "resource_id": 33,
          "text": "dan Yang menumbuhkan rerumputan,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5953,
      "verse_number": 5,
      "verse_key": "87:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ",
      "page_number": 591,
      "juz_number": 30,
      "translations": [
        {
          "id": 181028,
          "resource_id": 33,
          "text": "lalu dijadikan-Nya (rumput-rumput) itu kering kehitam-hitaman."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5954,
      "verse_number": 6,
      "verse_key": "87:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ",
      "page_number": 591,
      "juz_number": 30,
      "translations": [
        {
          "id": 181022,
          "resource_id": 33,
          "text": "Kami akan membacakan (Alquran) kepadamu (Muhammad) sehingga engkau tidak akan lupa,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5955,
      "verse_number": 7,
      "verse_key": "87:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ",
      "page_number": 591,
      "juz_number": 30,
      "translations": [
        {
          "id": 181023,
          "resource_id": 33,
          "text": "kecuali jika Allah menghendaki. Sungguh, Dia mengetahui yang terang dan yang tersembunyi."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5956,
      "verse_number": 8,
      "verse_key": "87:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَنُيَسِّرُكَ لِلْيُسْرَىٰ",
      "page_number": 591,
      "juz_number": 30,
      "translations": [
        {
          "id": 181024,
          "resource_id": 33,
          "text": "Dan Kami akan memudahkan bagimu ke jalan kemudahan (mencapai kebahagian dunia dan akhirat),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5957,
      "verse_number": 9,
      "verse_key": "87:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ",
      "page_number": 591,
      "juz_number": 30,
      "translations": [
        {
          "id": 181025,
          "resource_id": 33,
          "text": "oleh sebab itu berikanlah peringatan, karena peringatan itu bermanfaat,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5958,
      "verse_number": 10,
      "verse_key": "87:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "سَيَذَّكَّرُ مَن يَخْشَىٰ",
      "page_number": 591,
      "juz_number": 30,
      "translations": [
        {
          "id": 181883,
          "resource_id": 33,
          "text": "orang yang takut (kepada Allah) akan mendapat pelajaran,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5959,
      "verse_number": 11,
      "verse_key": "87:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيَتَجَنَّبُهَا ٱلْأَشْقَى",
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 181891,
          "resource_id": 33,
          "text": "dan orang yang celaka (kafir) akan menjauhinya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5960,
      "verse_number": 12,
      "verse_key": "87:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ",
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 181892,
          "resource_id": 33,
          "text": "(yaitu) orang yang akan memasuki api yang besar (neraka),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5961,
      "verse_number": 13,
      "verse_key": "87:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 181013,
          "resource_id": 33,
          "text": "selanjutnya dia di sana tidak mati dan tidak (pula) hidup."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5962,
      "verse_number": 14,
      "verse_key": "87:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قَدْ أَفْلَحَ مَن تَزَكَّىٰ",
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 181322,
          "resource_id": 33,
          "text": "Sungguh beruntung orang yang menyucikan diri (dengan beriman),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5963,
      "verse_number": 15,
      "verse_key": "87:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ",
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 181305,
          "resource_id": 33,
          "text": "dan mengingat nama Tuhannya, lalu dia salat."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5964,
      "verse_number": 16,
      "verse_key": "87:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 181299,
          "resource_id": 33,
          "text": "Sedangkan kamu (orang-orang kafir) memilih kehidupan dunia,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5965,
      "verse_number": 17,
      "verse_key": "87:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْـَٔاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ",
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 181102,
          "resource_id": 33,
          "text": "padahal kehidupan akhirat itu lebih baik dan lebih kekal."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5966,
      "verse_number": 18,
      "verse_key": "87:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ هَـٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ",
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 181100,
          "resource_id": 33,
          "text": "Sesungguhnya ini terdapat dalam kitab-kitab yang dahulu,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5967,
      "verse_number": 19,
      "verse_key": "87:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 531,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ",
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 181015,
          "resource_id": 33,
          "text": "(yaitu) kitab-kitab Ibrahim dan Musa."
        }
      ],
      "foot_notes": []
    }
  ]
}