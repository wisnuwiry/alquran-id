{
  "id": 79,
  "revelation_place": "makkah",
  "revelation_order": 81,
  "bismillah_pre": true,
  "name_simple": "An-Nazi'at",
  "name_complex": "An-Nāzi`āt",
  "name_arabic": "النازعات",
  "verses_count": 46,
  "pages": [
    583,
    584
  ],
  "slug": {
    "slug": "an-naziat",
    "locale": "en"
  },
  "translated_name": {
    "language_name": "indonesian",
    "name": "Malaikat yang Mencabut"
  },
  "verses": [
    {
      "id": 5713,
      "verse_number": 1,
      "verse_key": "79:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " وَٱلنَّـٰزِعَـٰتِ غَرْقًا",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180752,
          "resource_id": 33,
          "text": "Demi (malaikat) yang mencabut (nyawa) dengan keras."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5714,
      "verse_number": 2,
      "verse_key": "79:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلنَّـٰشِطَـٰتِ نَشْطًا",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180732,
          "resource_id": 33,
          "text": "Demi (malaikat) yang mencabut (nyawa) dengan lemah lembut."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5715,
      "verse_number": 3,
      "verse_key": "79:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلسَّـٰبِحَـٰتِ سَبْحًا",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180750,
          "resource_id": 33,
          "text": "Demi (malaikat) yang turun dari langit dengan cepat,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5716,
      "verse_number": 4,
      "verse_key": "79:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلسَّـٰبِقَـٰتِ سَبْقًا",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180744,
          "resource_id": 33,
          "text": "dan (malaikat) yang mendahului dengan kencang,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5717,
      "verse_number": 5,
      "verse_key": "79:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْمُدَبِّرَٰتِ أَمْرًا",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180754,
          "resource_id": 33,
          "text": "dan (malaikat) yang mengatur urusan (dunia).<sup foot_note=135846>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135846,
            "text": "* Dalam ayat <a href=\"79:1-5\">1-5</a> Allah bersumpah dengan malaikat-malaikat yang bermacam-macam sifat dan urusannya bahwa manusia akan dibangkitkan pada hari Kiamat. Sebagian mufasir berpendapat, bahwa dalam ayat-ayat ini, kecuali ayat <a href=\"79:5\">79:5</a>, Allah bersumpah dengan bintang-bintang.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5718,
      "verse_number": 6,
      "verse_key": "79:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ تَرْجُفُ ٱلرَّاجِفَةُ",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180803,
          "resource_id": 33,
          "text": "(Sungguh, kamu akan dibangkitkan) pada hari ketika tiupan pertama mengguncangkan alam,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5719,
      "verse_number": 7,
      "verse_key": "79:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَتْبَعُهَا ٱلرَّادِفَةُ",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180756,
          "resource_id": 33,
          "text": "(tiupan pertama) itu diiringi oleh tiupan kedua."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5720,
      "verse_number": 8,
      "verse_key": "79:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180758,
          "resource_id": 33,
          "text": "Hati manusia pada waktu itu merasa sangat takut,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5721,
      "verse_number": 9,
      "verse_key": "79:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَبْصَـٰرُهَا خَـٰشِعَةٌ",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 182675,
          "resource_id": 33,
          "text": "pandangannya tunduk."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5722,
      "verse_number": 10,
      "verse_key": "79:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَقُولُونَ أَءِنَّا لَمَرْدُودُونَ فِى ٱلْحَافِرَةِ",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180603,
          "resource_id": 33,
          "text": "(orang-orang kafir) berkata, \"Apakah kita benar-benar akan dikembalikan kepada kehidupan yang semula?<sup foot_note=135847>1</sup>"
        }
      ],
      "foot_notes": [
        {
          "foot_note": {
            "id": 135847,
            "text": "* Setelah orang-orang kafir mendengar adanya hari kebangkitan setelah mati mereka merasa heran dan mengejek sebab menurut keyakinan mereka tidak ada hari kebangkitan. Itulah sebabnya mereka bertanya.",
            "language_name": "indonesian",
            "language_id": 67
          }
        }
      ]
    },
    {
      "id": 5723,
      "verse_number": 11,
      "verse_key": "79:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَءِذَا كُنَّا عِظَـٰمًا نَّخِرَةً",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180711,
          "resource_id": 33,
          "text": "Apakah (akan dibangkitkan juga) apabila kita telah menjadi tulang belulang yang hancur?\""
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5724,
      "verse_number": 12,
      "verse_key": "79:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180720,
          "resource_id": 33,
          "text": "Mereka berkata, \"Kalau demikian, itu adalah suatu pengembalian yang merugikan.\""
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5725,
      "verse_number": 13,
      "verse_key": "79:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180712,
          "resource_id": 33,
          "text": "Maka pengembalian itu hanyalah dengan sekali tiupan saja."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5726,
      "verse_number": 14,
      "verse_key": "79:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180713,
          "resource_id": 33,
          "text": "Maka seketika itu mereka hidup kembali di bumi (yang baru)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5727,
      "verse_number": 15,
      "verse_key": "79:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَلْ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180714,
          "resource_id": 33,
          "text": "Sudahkah sampai kepadamu (Muhammad) kisah Musa?"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5728,
      "verse_number": 16,
      "verse_key": "79:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِذْ نَادَىٰهُ رَبُّهُۥ بِٱلْوَادِ ٱلْمُقَدَّسِ طُوًى",
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 180715,
          "resource_id": 33,
          "text": "Ketika Tuhan memanggilnya (Musa) di lembah suci yaitu Lembah Tuwa;"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5729,
      "verse_number": 17,
      "verse_key": "79:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180716,
          "resource_id": 33,
          "text": "pergilah engkau kepada Fir'aun! Sesungguhnya dia telah melampaui batas,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5730,
      "verse_number": 18,
      "verse_key": "79:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقُلْ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180717,
          "resource_id": 33,
          "text": "Maka katakanlah (kepada Fir'aun), \"Adakah keinginanmu untuk membersihkan diri (dari kesesatan),"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5731,
      "verse_number": 19,
      "verse_key": "79:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180718,
          "resource_id": 33,
          "text": "dan engkau akan kubimbing ke jalan Tuhanmu agar engkau takut kepada-Nya?\""
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5732,
      "verse_number": 20,
      "verse_key": "79:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَرَىٰهُ ٱلْـَٔايَةَ ٱلْكُبْرَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180721,
          "resource_id": 33,
          "text": "Lalu (Musa) memperlihatkan kepadanya mukjizat yang besar."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5733,
      "verse_number": 21,
      "verse_key": "79:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَكَذَّبَ وَعَصَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180722,
          "resource_id": 33,
          "text": "Tetapi dia (Fir'aun) mendustakan dan mendurhakai."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5734,
      "verse_number": 22,
      "verse_key": "79:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَدْبَرَ يَسْعَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180723,
          "resource_id": 33,
          "text": "Kemudian dia berpaling seraya berusaha menantang (Musa)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5735,
      "verse_number": 23,
      "verse_key": "79:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَحَشَرَ فَنَادَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180724,
          "resource_id": 33,
          "text": "Kemudian dia mengumpulkan (pembesar-pembesarnya) lalu berseru (memanggil kaumnya)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5736,
      "verse_number": 24,
      "verse_key": "79:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلْأَعْلَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180725,
          "resource_id": 33,
          "text": "(Seraya) berkata, \"Akulah tuhanmu yang paling tinggi.\""
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5737,
      "verse_number": 25,
      "verse_key": "79:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلْـَٔاخِرَةِ وَٱلْأُولَىٰٓ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180726,
          "resource_id": 33,
          "text": "Maka Allah menghukumnya dengan azab di akhirat dan siksaan di dunia."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5738,
      "verse_number": 26,
      "verse_key": "79:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ فِى ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰٓ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180727,
          "resource_id": 33,
          "text": "Sungguh, pada yang demikian itu terdapat pelajaran bagi orang yang takut (kepada Allah)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5739,
      "verse_number": 27,
      "verse_key": "79:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ءَأَنتُمْ أَشَدُّ خَلْقًا أَمِ ٱلسَّمَآءُ ۚ بَنَىٰهَا",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180728,
          "resource_id": 33,
          "text": "Apakah penciptaan kamu yang lebih hebat ataukah langit yang telah dibangun-Nya?"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5740,
      "verse_number": 28,
      "verse_key": "79:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "رَفَعَ سَمْكَهَا فَسَوَّىٰهَا",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180729,
          "resource_id": 33,
          "text": "Dia telah meninggikan bangunannya lalu menyempurnakannya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5741,
      "verse_number": 29,
      "verse_key": "79:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَىٰهَا",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180730,
          "resource_id": 33,
          "text": "dan Dia menjadikan malamnya (gelap gulita), dan menjadikan siangnya (terang benderang)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5742,
      "verse_number": 30,
      "verse_key": "79:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْأَرْضَ بَعْدَ ذَٰلِكَ دَحَىٰهَآ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 177551,
          "resource_id": 33,
          "text": "Dan setelah itu bumi Dia hamparkan."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5743,
      "verse_number": 31,
      "verse_key": "79:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعَىٰهَا",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180733,
          "resource_id": 33,
          "text": "Darinya Dia pancarkan mata air, dan (ditumbuhkan) tumbuh-tumbuhannya."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5744,
      "verse_number": 32,
      "verse_key": "79:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْجِبَالَ أَرْسَىٰهَا",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180734,
          "resource_id": 33,
          "text": "Dan gunung-gunung Dia pancangkan dengan kokoh."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5745,
      "verse_number": 33,
      "verse_key": "79:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180735,
          "resource_id": 33,
          "text": "(Semua itu) untuk kesenanganmu dan untuk hewan-hewan ternakmu."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5746,
      "verse_number": 34,
      "verse_key": "79:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلْكُبْرَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180736,
          "resource_id": 33,
          "text": "Maka apabila malapetaka besar (hari Kiamat) telah datang,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5747,
      "verse_number": 35,
      "verse_key": "79:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ يَتَذَكَّرُ ٱلْإِنسَـٰنُ مَا سَعَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180737,
          "resource_id": 33,
          "text": "yaitu pada hari (ketika) manusia teringat akan apa yang telah dikerjakannya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5748,
      "verse_number": 36,
      "verse_key": "79:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَبُرِّزَتِ ٱلْجَحِيمُ لِمَن يَرَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180738,
          "resource_id": 33,
          "text": "dan neraka diperlihatkan dengan jelas kepada setiap orang yang melihat."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5749,
      "verse_number": 37,
      "verse_key": "79:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَمَّا مَن طَغَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180742,
          "resource_id": 33,
          "text": "Maka adapun orang yang melampaui batas,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5750,
      "verse_number": 38,
      "verse_key": "79:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَءَاثَرَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180739,
          "resource_id": 33,
          "text": "dan lebih mengutamakan kehidupan dunia,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5751,
      "verse_number": 39,
      "verse_key": "79:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِنَّ ٱلْجَحِيمَ هِىَ ٱلْمَأْوَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180740,
          "resource_id": 33,
          "text": "maka sungguh, nerakalah tempat tinggalnya."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5752,
      "verse_number": 40,
      "verse_key": "79:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفْسَ عَنِ ٱلْهَوَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180743,
          "resource_id": 33,
          "text": "Dan adapun orang-orang yang takut kepada kebesaran Tuhannya dan menahan diri dari (keinginan) hawa nafsunya,"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5753,
      "verse_number": 41,
      "verse_key": "79:41",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِنَّ ٱلْجَنَّةَ هِىَ ٱلْمَأْوَىٰ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 178289,
          "resource_id": 33,
          "text": "maka sungguh, surgalah tempat tinggal-(nya)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5754,
      "verse_number": 42,
      "verse_key": "79:42",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَسْـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرْسَىٰهَا",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180745,
          "resource_id": 33,
          "text": "Mereka (orang-orang kafir) bertanya kepadamu (Muhammad) tentang hari Kiamat, \"Kapankah terjadinya?\""
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5755,
      "verse_number": 43,
      "verse_key": "79:43",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِيمَ أَنتَ مِن ذِكْرَىٰهَآ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180746,
          "resource_id": 33,
          "text": "Untuk apa engkau perlu menyebutkannya (waktunya)?"
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5756,
      "verse_number": 44,
      "verse_key": "79:44",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180747,
          "resource_id": 33,
          "text": "Kepada Tuhanmulah (dikembalikan) kesudahannya (ketentuan waktunya)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5757,
      "verse_number": 45,
      "verse_key": "79:45",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخْشَىٰهَا",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180748,
          "resource_id": 33,
          "text": "Engkau (Muhammad) hanyalah pemberi peringatan bagi siapa yang takut kepadanya (hari Kiamat)."
        }
      ],
      "foot_notes": []
    },
    {
      "id": 5758,
      "verse_number": 46,
      "verse_key": "79:46",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوٓا۟ إِلَّا عَشِيَّةً أَوْ ضُحَىٰهَا",
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 180749,
          "resource_id": 33,
          "text": "Pada hari ketika mereka melihat hari Kiamat itu (karena suasananya hebat), mereka merasa seakan-akan hanya (sebentar saja) tinggal (di dunia) pada waktu sore atau pagi hari."
        }
      ],
      "foot_notes": []
    }
  ]
}